{
	"runtime": {
		"arguments": "--v=1 --inspect",
		"version": "canary"
	},
	"startup_app": {
		"uuid": "foo",
		"url": "http://localhost:5050/app.html",
		"contentNavigation": {
			"_allowlist": ["http://example.com"],
			"_denylist": ["http://example.com"],
			"_whitelist": ["http://example.com", "http://localhost:5050/app.html"],
			"_blacklist": ["https://example.org/foo/bar.html"]
		},
		"autoShow": true
	},
	"_platform": {
		"icon": "http://localhost:5050/assets/icon.png",
		"uuid": "openfin-platform-template-ts",
		"autoShow": true,
		"providerUrl": "http://localhost:5050/provider.html",
		"apiDiagnostics": false,
		"defaultWindowOptions": {
			"saveWindowState": false,
			"showBackgroundImages": true,
			"backgroundThrottling": true,
			"fdc3InteropApi": "1.2"
		},
		"defaultViewOptions": {
			"fdc3InteropApi": "1.2",
			"contentNavigation": {
				"_allowlist": ["http://localhost:5050/app.html"],
				"_denylist": ["https://www.snapple.com/products"],
				"whitelist": ["https://whitelisted.example"],
				"_blacklist": ["https://example.org/foo/bar.html"]
			}
		},
		"interopBrokerConfiguration": {
			"logging": {
				"beforeAction": {
					"enabled": false
				},
				"afterAction": {
					"enabled": false
				}
			}
		}
	},
	"_snapshot": {
        "windows": [
            {
                "name": "openfin-platform-template-window",
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
										"name": "foo",
                                        "url": "http://localhost:5050/app.html"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
